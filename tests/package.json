{
  "name": "operator-tests",
  "version": "1.0.0",
  "description": "Comprehensive test suite for Operator demos",
  "type": "module",
  "scripts": {
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest --config jest.config.js",
    "test:e2e": "node e2e-playwright.test.js",
    "test:e2e:headed": "HEADLESS=false node e2e-playwright.test.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "testing",
    "playwright",
    "react",
    "unit-tests",
    "e2e-tests"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "playwright": "^1.40.0"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@babel/preset-react": "^7.23.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
